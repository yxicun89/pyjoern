{
  "source_files": [
    "whiletest.py"
  ],
  "output_directory": "graph_outputs",
  "visualization": {
    "figure_size": [
      12,
      8
    ],
    "node_size": 2000,
    "font_size": 10,
    "dpi": 300,
    "save_graphs": true,
    "show_graphs": true
  },
  "colors": {
    "cfg": {
      "entry": "#90EE90",
      "exit": "#FFB6C1",
      "merge": "#FFD700",
      "regular": "#87CEEB"
    },
    "ast": {
      "function": "#98FB98",
      "call": "#DDA0DD",
      "assign": "#F0E68C",
      "default": "#B0C4DE"
    },
    "ddg": {
      "default": "#FFA07A"
    }
  },
  "layouts": {
    "cfg": "hierarchical",
    "ast": "spring",
    "ddg": "spring"
  },
  "advanced_visualization": {
    "flowchart": {
      "enabled": true,
      "description": "標準的なフローチャート形式",
      "size": [16, 12],
      "node_size": 4000,
      "colors": {
        "start": "#2ECC71",
        "end": "#E74C3C",
        "process": "#3498DB",
        "decision": "#F39C12",
        "merge": "#9B59B6",
        "loop": "#1ABC9C",
        "edge": "#34495E"
      }
    },
    "timeline": {
      "enabled": true,
      "description": "実行順序を時系列で表示",
      "size": [18, 8],
      "step_width": 2.0
    },
    "overview": {
      "enabled": true,
      "description": "主要処理のみを簡潔に表示",
      "size": [12, 8],
      "simplify_threshold": 5
    }
  },
  "node_labels": {
    "language": "japanese",
    "show_details": true,
    "max_statement_length": 25,
    "include_statement_count": true,
    "symbols": {
      "start": "🚀",
      "end": "🏁",
      "process": "⚙️",
      "decision": "❓",
      "merge": "🔄",
      "loop": "🔁"
    }
  },
  "analysis_options": {
    "generate_reports": true,
    "include_statistics": true,
    "complexity_thresholds": {
      "low": 5,
      "medium": 10,
      "high": 15
    }
  }
}